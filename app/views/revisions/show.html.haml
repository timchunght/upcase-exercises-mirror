%header.solutions-main
  %nav.participants
    = render "my_solution",
      solution: @review.submitted_solution,
      solutions: @review.solutions,
      exercise: @review.exercise

  %section.next-steps
    = render @review.status

%div.main-inner
  = render "solutions/revision_history", feedback: @review.feedback

  %nav.participants
    = render "peer_solutions", solutions: @review.solutions

  = render "revisions/feedback", feedback: @review.feedback
